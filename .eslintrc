{
  "env": {
    "browser": true,
    "jest": true,
    "es2021": true
  },
  "plugins": ["react", "jest"],
  "extends": [
    "react-app",
    "react-app/jest",
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "warn",
    "no-eval": "error",
    "react/prop-types": "off"
  }
}
